
apiVersion: apps/v1
kind: Deployment
metadata:
  name: nginx-deployment-2
  labels:
    app: nginx-deployment-2
    evaluation: test
spec:
  replicas: 1
  selector:
    matchLabels:
      app: nginx-deployment-2
      evaluation: test
  template:
    metadata:
      labels:
        app: nginx-deployment-2
        evaluation: test
    spec:
      containers:
      - image: nginx
        name: nginx-deployment-2

---


